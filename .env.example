# Environment Variables Template
# Copy this file to .env.local and fill in your values

# GitHub Configuration
REACT_APP_GITHUB_OWNER=wissamyah
REACT_APP_GITHUB_REPO=suprememanagement
REACT_APP_GITHUB_BRANCH=data
REACT_APP_GITHUB_DATA_PATH=data/inventory.json

# Security
# Generate a secure key for production use
REACT_APP_ENCRYPTION_KEY=your-secure-encryption-key-here

# API Configuration
REACT_APP_API_BASE_URL=https://api.github.com

# Feature Flags (optional)
REACT_APP_ENABLE_AUTO_SYNC=true
REACT_APP_ENABLE_ENCRYPTION=true
REACT_APP_ENABLE_OFFLINE_MODE=true

# Development
REACT_APP_DEBUG_MODE=false